{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": "error",
        "comma-style": "off",
        "semi": "off",
        "jsx-quotes": "off",
        "indent": "off",
        "no-undef": "off",
        "no-var": "warn",
        "react/prop-types": [{
            "ignore": "ignore"
        }]
    }
}
